{
    "name": "lciolecki/zf-extensions-library",
    "author": "Lukasz Ciolecki",
    "description": "Extensions library for Zend Framework v1",
    "type": "library",
    "keywords": [
        "php",
        "extlib",
        "Zend Framework"
    ],
    "homepage": "",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=5.3.3"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "http://github.com/lciolecki/cron-jobs-manager"
        }
    ],
    "require-dev": {
        "lciolecki/php-library": "master-dev",
        "zendframework/zendframework1": "master"
    },
    "autoload": {
        "psr-0": {
            "Extlib\\": "library"
        }
    },
    "include-path": [
        "library/"
    ]
}
