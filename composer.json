{
    "name": "lciolecki/zf-extensions-library",
    "author": "Lukasz Ciolecki",
    "description": "Extensions library for Zend Framework v1",
    "type": "library",
    "keywords": [
        "php",
        "extlib",
        "Zend Framework"
    ],
    "homepage": "",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=5.3.3"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/lciolecki/php-library"
        }
    ],
    "require": {
        "lciolecki/php-library": "dev-master",
        "zendframework/zendframework1": "1.*"
    },
    "autoload": {
        "psr-0": {
            "Extlib_Log_Writer": "library/Extlib/Log/Writer/", 
            "Extlib\\": "library/",
            "ExtlibTest\\": "tests/"
        }
    }
}
